          "goal": "Stop the scroll"
        },
        {
          "timestamp": "3-10s",
          "section": "Interest",
          "visual": "Show the problem/pain point",
          "audio": "Agitate the pain - make them feel it",
          "text_overlay": "Relatable struggle",
          "goal": "Create emotional connection"
        },
        {
          "timestamp": "10-25s",
          "section": "Desire",
          "visual": "Show the solution/transformation",
          "audio": "Introduce the NEW mechanism",
          "text_overlay": "The unique solution",
          "goal": "Build desire for solution"
        },
        {
          "timestamp": "25-35s",
          "section": "Proof",
          "visual": "Results, testimonials, social proof",
          "audio": "Statistics and success stories",
          "text_overlay": "Numbers and outcomes",
          "goal": "Establish credibility"
        },
        {
          "timestamp": "35-45s",
          "section": "Action",
          "visual": "Clear CTA with urgency",
          "audio": "Direct call to action with scarcity",
          "text_overlay": "CTA + urgency element",
          "goal": "Drive immediate action"
        }
      ],
      "key_principles": [
        "Every second must earn the next second",
        "Show don't tell",
        "Create NEW mechanism (not commodity)",
        "Use specific numbers and timeframes",
        "Build urgency without pressure"
      ]
