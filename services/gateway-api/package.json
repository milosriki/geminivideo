{
  "name": "gateway-api",
  "version": "1.0.0",
  "description": "Gateway API with Scoring Engine",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "worker:safe-executor": "ts-node src/workers/safe-executor-worker.ts",
    "worker:safe-executor:prod": "node dist/workers/safe-executor-worker.js",
    "worker:self-learning": "ts-node -e \"import('./src/workers/self-learning-cycle').then(m => { const pg = require('pg'); const pool = new pg.Pool({ connectionString: process.env.DATABASE_URL }); m.startSelfLearningCycleWorker(pool); })\"",
    "worker:batch": "ts-node src/jobs/batch-executor.ts",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^5.19.0",
    "@sentry/node": "^7.100.0",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "prisma": "^5.19.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}