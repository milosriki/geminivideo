{
  "dashboard": {
    "title": "AI API Costs Dashboard",
    "tags": ["ai", "costs", "ml", "tokens"],
    "timezone": "utc",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Total Cost (Last 24h)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(ai_api_cost_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "Cost Per Hour",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(ai_api_cost_total[1h])) * 3600",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "title": "Total Tokens (Last 24h)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(ai_api_tokens_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(ai_cache_hits_total[5m])) / (sum(rate(ai_cache_hits_total[5m])) + sum(rate(ai_cache_misses_total[5m]))) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 5,
        "title": "Cost by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "sum(rate(ai_api_cost_total[5m])) by (provider) * 3600",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "sum"]}
        }
      },
      {
        "id": 6,
        "title": "Cost by Model",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "sum(rate(ai_api_cost_total[5m])) by (model) * 3600",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "sum"]}
        }
      },
      {
        "id": 7,
        "title": "Token Usage (Input vs Output)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "sum(rate(ai_api_tokens_total{token_type=\"input\"}[5m])) by (provider)",
            "legendFormat": "Input - {{provider}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(ai_api_tokens_total{token_type=\"output\"}[5m])) by (provider)",
            "legendFormat": "Output - {{provider}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 8,
        "title": "API Call Rate by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "targets": [
          {
            "expr": "sum(rate(ai_api_calls_total[5m])) by (provider)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 9,
        "title": "AI API Latency by Model",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ai_api_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "P95 - {{model}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(ai_api_duration_seconds_bucket[5m])) by (le, model))",
            "legendFormat": "P99 - {{model}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 10,
        "title": "AI API Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "sum(rate(ai_api_errors_total[5m])) by (provider, error_type)",
            "legendFormat": "{{provider}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"]}
        }
      },
      {
        "id": 11,
        "title": "Cost Distribution (Last 24h)",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "sum(increase(ai_api_cost_total[24h])) by (model)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "percent"]},
          "pieType": "donut",
          "displayLabels": ["percent"]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 12,
        "title": "Cache Savings (Estimated)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 30},
        "targets": [
          {
            "expr": "sum(increase(ai_cache_hits_total[24h])) * 0.015",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 13,
        "title": "Most Expensive Operations (Top 10)",
        "type": "table",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 30},
        "targets": [
          {
            "expr": "topk(10, sum(rate(ai_api_cost_total[1h])) by (operation, model) * 3600)",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {
                "operation": "Operation",
                "model": "Model",
                "Value": "Cost/Hour ($)"
              }
            }
          }
        ],
        "options": {
          "sortBy": [{"displayName": "Cost/Hour ($)", "desc": true}]
        }
      }
    ]
  }
}
