# =============================================================================
# Gemini Video - Environment Configuration Template
# =============================================================================
# SECURITY: Never commit actual credentials to version control
# Copy this file to .env and fill in your actual values
# =============================================================================

# -----------------------------------------------------------------------------
# AI Model API Keys (Required)
# -----------------------------------------------------------------------------
GEMINI_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Gemini Model Configuration
GEMINI_MODEL_ID=gemini-2.0-flash-thinking-exp-1219

# Voice Generation (Optional - for AI voiceovers)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
# OpenAI API key (same as above) is used for OpenAI TTS

# -----------------------------------------------------------------------------
# AI Video Generation (2025 Next-Gen - COMPETITIVE ADVANTAGE)
# -----------------------------------------------------------------------------
# Runway Gen-3 Alpha (AVAILABLE NOW - recommended for production)
# Get API key: https://runwayml.com/
RUNWAY_API_KEY=your_runway_api_key_here

# Kling AI (Good for realistic motion and human subjects)
# Get API key: https://klingai.com/
KLING_API_KEY=your_kling_api_key_here

# Pika Labs (Fast generation, perfect for drafts and iterations)
# Get API key: https://pika.art/
PIKA_API_KEY=your_pika_api_key_here

# Note: OpenAI Sora uses OPENAI_API_KEY above when available (limited access)

# -----------------------------------------------------------------------------
# Database Configuration (Required)
# -----------------------------------------------------------------------------
# Supabase (Primary Database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_DB_PASSWORD=your_db_password

# PostgreSQL connection string (Derived from Supabase)
# Format: postgresql://postgres:[PASSWORD]@[HOST]:5432/postgres
DATABASE_URL=postgresql://postgres:password@db.project.supabase.co:5432/postgres

# Redis connection string (Optional - defaults to localhost if not set)
# REDIS_URL=redis://localhost:6379

# -----------------------------------------------------------------------------
# Service URLs (Required for Production)
# -----------------------------------------------------------------------------
# Gateway API (main entry point)
GATEWAY_API_URL=http://localhost:8000

# Internal microservices
DRIVE_INTEL_URL=http://localhost:8001
VIDEO_AGENT_URL=http://localhost:8002
ML_SERVICE_URL=http://localhost:8003
META_PUBLISHER_URL=http://localhost:8083

# Titan-Core service (if running separately)
TITAN_CORE_URL=http://localhost:8004

# -----------------------------------------------------------------------------
# Meta Ads Integration (Required for Meta Publisher)
# -----------------------------------------------------------------------------
META_ACCESS_TOKEN=your_meta_access_token
META_AD_ACCOUNT_ID=act_1234567890
META_PAGE_ID=1234567890
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_CLIENT_TOKEN=your_meta_client_token
META_SANDBOX_TOKEN=optional_sandbox_token

# -----------------------------------------------------------------------------
# Firebase Configuration (Frontend)
# -----------------------------------------------------------------------------
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# -----------------------------------------------------------------------------
# Google Cloud Platform (Optional)
# -----------------------------------------------------------------------------
# For Google Drive integration and Cloud Storage
GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_JSON=/path/to/service-account.json
GCS_BUCKET_NAME=your-gcs-bucket-name

# -----------------------------------------------------------------------------
# Application Configuration
# -----------------------------------------------------------------------------
# Environment: development, staging, production
NODE_ENV=development
VITE_ENV=development

# Port configuration (optional overrides)
PORT=8000
GATEWAY_PORT=8000
DRIVE_INTEL_PORT=8001
VIDEO_AGENT_PORT=8002
ML_SERVICE_PORT=8003
META_PUBLISHER_PORT=8083

# -----------------------------------------------------------------------------
# Feature Flags (Optional)
# -----------------------------------------------------------------------------
ENABLE_ANALYTICS=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_MCP_INTEGRATION=false

# -----------------------------------------------------------------------------
# Shared Config Path (Optional)
# -----------------------------------------------------------------------------
# Path to shared configuration directory
CONFIG_PATH=./shared/config

# -----------------------------------------------------------------------------
# Development Settings (Optional)
# -----------------------------------------------------------------------------
# Enable debug logging
DEBUG=false
LOG_LEVEL=info

# Hot reload for knowledge base (seconds)
KNOWLEDGE_RELOAD_INTERVAL=300

# -----------------------------------------------------------------------------
# Security Settings (Optional)
# -----------------------------------------------------------------------------
# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# JWT secret for authentication (if implemented)
JWT_SECRET=your_jwt_secret_here

# -----------------------------------------------------------------------------
# Video Processing (Optional)
# -----------------------------------------------------------------------------
# Maximum video file size in MB
MAX_VIDEO_SIZE_MB=500

# Temporary storage path
TEMP_STORAGE_PATH=/tmp/geminivideo

# Output storage path
OUTPUT_STORAGE_PATH=/tmp/geminivideo/output

# Voiceover storage path
VOICEOVER_DIR=/tmp/geminivideo/voiceovers

# Asset library path
ASSET_DIR=/tmp/geminivideo/assets

# -----------------------------------------------------------------------------
# ML Model Configuration (Optional)
# -----------------------------------------------------------------------------
# Minimum samples required before updating weights
MIN_SAMPLES_FOR_UPDATE=50

# Learning rate for weight updates
LEARNING_RATE=0.01

# Maximum weight change per update
MAX_WEIGHT_DELTA=0.1

# -----------------------------------------------------------------------------
# Thompson Sampling / A/B Testing (Optional)
# -----------------------------------------------------------------------------
# Enable Thompson Sampling for variant selection
ENABLE_THOMPSON_SAMPLING=true

# Minimum conversions before declaring winner
MIN_CONVERSIONS_FOR_WINNER=30

# -----------------------------------------------------------------------------
# Deployment Configuration (Production)
# -----------------------------------------------------------------------------
# Cloud Run / Container deployment settings
CLOUD_RUN_REGION=us-central1
CLOUD_RUN_SERVICE_ACCOUNT=your-service-account@project.iam.gserviceaccount.com

# Vercel deployment (frontend)
VERCEL_TOKEN=your_vercel_token

# Supabase (if using instead of PostgreSQL)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
